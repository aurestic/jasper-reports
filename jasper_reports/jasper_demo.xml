<?xml version="1.0"?>
<openerp>
    <data>

        <record id="res_users_jaspertest" model="ir.actions.report.xml">
            <!-- <field name="string">Jasper Users</field> -->
            <field name="auto" eval="True"/>
            <field name="model">res.users</field>
            <field name="jasper_model_id" ref="base.model_res_users"/>
            <field name="name">Jasper Users</field>
            <field name="report_name">res.user.jaspertest</field>
            <!-- <field name="rml">jasper_reports/demo/user.jrxml</field> -->
            <field name="report_rml">jasper_reports/demo/user.jrxml</field>
            <!-- <field name="menu" eval="True"/> -->
            <field name="header" eval="False"/>
            <field name="jasper_report" eval="True"/>
            <field name="jasper_output">pdf</field>
        </record>

        <record id="res_users_jaspertest_values" model="ir.values">
            <field name="name">Jasper Users</field>
            <field name="model">res.users</field>
            <field name="key2">client_print_multi</field>
            <field name="value" eval="'ir.actions.report.xml,%d'%res_users_jaspertest"/>
        </record>

        <record id="res_partner_jaspertest" model="ir.actions.report.xml">
            <!-- <field name="string">Jasper Partners</field> -->
            <field name="auto" eval="True"/>
            <field name="model">res.partner</field>
            <field name="jasper_model_id" ref="base.model_res_partner"/>
            <field name="name">Jasper Partners</field>
            <field name="report_name">res.partner.jaspertest</field>
            <!-- <field name="rml">jasper_reports/demo/partner.jrxml</field> -->
            <field name="report_rml">jasper_reports/demo/partner.jrxml</field>
            <!-- <field name="menu" eval="True"/> -->
            <field name="header" eval="False"/>
            <field name="jasper_report" eval="True"/>
            <field name="jasper_output">pdf</field>
        </record>

        <record id="res_partner_jaspertest_values" model="ir.values">
            <field name="name">Jasper Partners</field>
            <field name="model">res.partner</field>
            <field name="key2">client_print_multi</field>
            <field name="value" eval="'ir.actions.report.xml,%d'%res_partner_jaspertest"/>
        </record>

    </data>
</openerp>
